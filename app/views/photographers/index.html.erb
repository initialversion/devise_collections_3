<div class="page-header">
  <h1>
    Photographers
    <a href="/photographers/new" class="btn btn-lg btn-success">New Photographer</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Email</th>
        <th>Password</th>
        <th>Nickname</th>
        <th>Actions</th>
      </tr>

      <% @photographers.each do |photographer| %>
      <tr>
        <td><%= photographer.email %></td>
        <td><%= photographer.password %></td>
        <td><%= photographer.nickname %></td>
        <td>
          <a href="/photographers/<%= photographer.id %>" class="btn btn-primary">Show</a>
          <a href="/photographers/<%= photographer.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_photographer/<%= photographer.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
